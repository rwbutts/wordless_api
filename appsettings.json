{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  "Kestrel":{
     "Endpoints":{
        "Http":{
           "Url":"http://*:5000",
        }
     },
     "Cors": {
          "PolicyName": "Default",
          "AllowedOrigins": [ "*" ],
          "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
          "AllowedHeaders": [ "*" ],
          "AllowCredentials": false
     }
  }
}
